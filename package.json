{
  "name": "@gatsbystorefront/gatsby-source-shopify",
  "version": "3.6.0-1",
  "description": "Gatsby source plugin for building websites using Gatsby Storefront API for Shopify as a data source.",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "npm run build -- --watch"
  },
  "homepage": "https://github.com/GatsbyStorefront/gatsby-source-shopify#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/GatsbyStorefront/gatsby-source-shopify.git"
  },
  "bugs": {
    "url": "https://github.com/GatsbyStorefront/gatsby-source-shopify/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "GatsbyStorefront",
    "shopify"
  ],
  "author": {
    "name": "Pavel Ivanov",
    "email": "paveliv@gmail.com",
    "url": "https://gatsbystorefront.com/"
  },
  "license": "MPL-2.0",
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "cross-env": "^7.0.2"
  },
  "dependencies": {
    "babel-preset-gatsby-package": "^0.8.0-next.0",
    "chalk": "^4.1.0",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-source-filesystem": "^2.7.0-next.1",
    "graphql-request": "^3.3.0",
    "lodash": "^4.17.20",
    "p-iteration": "^1.1.8",
    "prettyjson": "^1.2.1"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "index.js"
}
